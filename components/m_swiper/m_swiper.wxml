<!--components/m_swiper/m_swiper.wxml-->
<swiper class="swiper"
  circular
  autoplay
  interval="2000"
  duration="200"
  indicator-dots
  indicator-color="#557777"
  indicator-active-color="red">
  <block wx:for="{{list}}" wx:key="index">
    <swiper-item class="swiper-item">
      <image src="{{item.url}}" style="width:100%" mode="widthFix"/>
    </swiper-item>
  </block>
</swiper>

